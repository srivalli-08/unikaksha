#Budget
v=int(input())
u=int(input())
w=int(input())
x=int(input())
y=int(input())
p=v*350.34
q=u*230.34
r=w*190.55
s=x*125.30
t=y*180.90
z=p+q+r+s+t
if z<=15000:
    print("yes")
else:
    print("no")
    
#Sece Dining
x=input()
y=int(input())
if x=='front' and y==1 or x=='rear' and y==2:
    print("Left handed")
elif x=='front' and y==2 or x=='rear' and y==1:
    print("Right handed")
else:
    print("Invalid Input")
    
#Intersection of a Circle
import math
x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
d=math.sqrt((x1-x2)**2+(y1-y2)**2)
if d==r1+r2:
    print("The circles are tangential")
elif d<r1+r2:
    print("The circles overlap")
else:
    print("The circles do not overlap")
    
#Microwave Oven
print("Enter the number of items")
A=int(input())
print("Enter the single item heating time ")
B=float(input())
C=B+(50/100)*B
D=B+2*B
if A==2:
    print("The recommended heating time is %.2f"%C)
elif A==3:
    print("The recommended heating time is %.2f"%D)
else:
    print("Number of items is more")



