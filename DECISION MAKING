#Greatest of two numbers
a=int(input())
b=int(input())
if a<b:
    print(a,"is less than",b)
elif a>b:
    print(a,"is greater than",b)
else: 
    print(a,"is equal to",b)

#Vowel Or Consonants
s=input()
if s in "aeiou":
    print("vowel")
elif s in "bcdfghjklmnpqrstvwxyz":
    print("consonant")
else:
    print("not an alphabet")
    
#Grading System
marks=int(input())
if marks==100:
    print("grade-S")
elif marks>=90 and marks<=100:
    print("grade-A")
elif marks>=80 and marks<=90:
    print("grade-B")
elif marks>=70 and marks<=80:
    print("grade-C")
elif marks>=60 and marks<=70:
    print("grade-D")
elif marks>=50 and marks<=60:
    print("grade-E")
elif marks<=50:
    print("grade-F")
else:
    print("invalid input")
    
#Profit or Loss
x=float(input())
y=float(input())
z=y*12
print("Enter the price of  a dozen mangoes")
print("Enter the price at which 1 mango is being sold")
if (z<x):
    print("Loss:Rs.%.2f"%(x-z))
elif (z>x):
    print("Profit:Rs.%.2f"%(z-x))
else: 
    print("No profit nor loss")
    
#Fee Collection
print("Enter the student type")
s=input()
if s=="MSDS":
    print("Enter tuition fee")
    T=float(input())
    print("Enter bus fee")
    B=float(input())
    x=T+B
    print("The fees to be paid by the student is Rs.","%.2f"%x)
elif s=="MSH":
     print("Enter tuition fee")
     T=float(input())
     print("Enter hostel fee")
     H=float(input())
     y=T+H
     print("The fees to be paid by the student is Rs.","%.2f"%y)
elif s=="MGSDS":
      print("Enter tuition fee")
      T=float(input())
      print("Enter bus fee")
      B=float(input())
      z=((150/100)*T)+B
      print("The fees to be paid by the student is Rs.","%.2f"%z)
elif s=="MGSH":
      print("Enter tuition fee")
      T=float(input())
      print("Enter hostel fee")
      H=float(input())
      p=((150/100)*T)+H
      print("The fees to be paid by the student is Rs.","%.2f"%p)

#Y2K Problem Detector
print("Enter Year of Birth\n")
by=int(input())
print("Enter Current year\n")
cy=int(input())
if(cy<by):
    by=100-by
    x=by+cy
    print("Your age is",x)
else:
     print("Your age is",cy-by)
     
#Lab Allocation
int("enter x")
L1=int(input())
print("enter y")
L2=int(input())
print("enter z")
